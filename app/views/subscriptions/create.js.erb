<% if @subscription.errors.present?%>
			<% @subscription.errors.full_messages.each do |message|%>
			$('.subscription_errors').html('<%= j message %>').css('color','red');
		<%end%>
	<%else%>
	$('.follow-button').hide();
  l= "<%= j link_to 'Unfollow', "#{subscription_path(@subscription)}", method: :delete, remote: :true, class: 'btn btn-danger unfollow-button',role:'button'%>";
  $('.follow_links').html(l);
	<%end%>
